{
  "manifest_version": 3,
  "name": "Unpaywall",
  "version": "1.0",
  "permissions": [
    "contextMenus",
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "https://archive.today/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
